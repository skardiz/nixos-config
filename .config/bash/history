waydroid show-full-ui
sudo waydroid shell
cd nixos-config
tree
publish 
sudo nixos-rebuild switch --flake .#shershulya
sudo waydroid init
sudo waydroid init -s GAPPS -f
sudo nano /var/lib/waydroid/waydroid_base.prop
sudo systemctl start waydroid-container
waydroid session start
sudo systemctl enable waydroid-container.service
sudo systemctl disable waydroid-container.service
sudo nixos-rebuild switch --flake .#shershulya
cd nixos-config
sudo nixos-rebuild switch --flake .#shershulya
sudo nixos-rebuild switch --flake .#shershulya
sudo nixos-rebuild switch --flake .#shershulya
publish 
sudo nixos-rebuild switch --flake .#shershulya
reboot 
# Останавливаем все, что может работать
sudo systemctl stop waydroid-container.service
waydroid session stop
# ВНИМАНИЕ: ЭТА КОМАНДА УДАЛИТ ВСЕ ПРИЛОЖЕНИЯ И ДАННЫЕ WAYDROID
# Удаляем старые данные, чтобы начать с чистого листа
sudo rm -rf /var/lib/waydroid /home/.waydroid ~/waydroid ~/.share/waydroid ~/.local/share/applications/*aydroid* ~/.local/share/waydroid
# Инициализируем WayDroid со встроенными сервисами Google
sudo waydroid init -s GAPPS -f
sudo mkdir -p /home/shared/waydroid
sudo chown alex:users /home/shared/waydroid # Даем права на папку
sudo chmod 775 /home/shared/waydroid
cd nixos-config
sudo nixos-rebuild switch --flake .#shershulya
publish 
sudo nixos-rebuild switch --flake .#shershulya
reboot 
